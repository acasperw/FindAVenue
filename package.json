{
  "name": "whit-test",
  "version": "1.0.0",
  "description": "Test for FindAVenue",
  "main": "src/main.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint src/js/components/**",
    "watch": "watchify -v -d -t [ babelify --presets [ es2015 react ] ] src/js/main.js -o dist/js/compiled.js",
    "build": "NODE_ENV=production browserify -t [ babelify --presets [ es2015 react ] ] src/js/main.js | uglifyjs > dist/js/compiled.js | lessc --clean-css src/assets/less/styles.less dist/css/styles.min.css"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "author": "Alex Whitlock",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "classnames": "^2.2.5",
    "moment": "^2.14.1",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-datetime": "^2.4.0",
    "react-dom": "*",
    "react-router": "^2.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-jest": "^13.2.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^9.0.8",
    "envify": "^3.4.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "jest-cli": "^13.2.3",
    "less": "*",
    "less-plugin-clean-css": "*",
    "react-addons-test-utils": "^15.2.1",
    "react-shallow-testutils": "^2.0.0",
    "uglify-js": "^2.4.20",
    "watchify": "^3.1.2"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ]
  }
}
